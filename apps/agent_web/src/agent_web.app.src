{application, agent_web,
 [{description, "Agent System Web Interface"},
  {vsn, "0.2.0"},
  {registered, []},
  {mod, {agent_web_app, []}},
  {applications,
   [kernel,
    stdlib,
    ssl,
    crypto,
    public_key,
    gproc,
    mnesia,
    agents,
    openai,
    cowboy,
    jsx,
    jiffy,
    uuid,
    gun,
    hackney,
    yamerl,
    %% sqlite3,  % Commented due to build issues
    jesse,
    prometheus,
    prometheus_httpd,
    prometheus_cowboy,
    jwt,
    base64url,
    poolboy
   ]},
  {env,[
   {port, 8080},
   {static_dir, "priv/static"},
   {templates_dir, "priv/templates"}
  ]},
  {modules, []},
  {licenses, ["MIT"]},
  {maintainers, ["Arthur Colle"]},
  {links, [
   {"GitHub", "https://github.com/arthurcolle/agents.erl"},
   {"Documentation", "https://agents.erl.sh"}
  ]}
 ]}.